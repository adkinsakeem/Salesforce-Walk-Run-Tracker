<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Distance_Ran__c</fullName>
    <description>This shows the distance ran with the word &quot;miles&quot; in front of it.</description>
    <externalId>false</externalId>
    <formula>IF(Distance__c &gt; 0,TEXT(Distance__c) &amp;  &quot; Miles&quot;, &quot;N/A&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Distance Ran</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
